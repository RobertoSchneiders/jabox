<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>org.jabox</groupId>
	<artifactId>jabox</artifactId>
	<packaging>pom</packaging>
	<version>1.0.0-SNAPSHOT</version>
	<!-- TODO project name  -->
	<name>Jabox</name>
	<description/>

	<modules>
		<module>jabox-webapp</module>
		<module>jabox-persistence</module>
		<module>jabox-site</module>
		<module>scm-subversion</module>
		<module>bts-redmine</module>
		<module>bts-bugzilla</module>
		<module>bts-jtrac</module>
		<module>cis-hudson</module>
		<module>mrm-nexus</module>
		<module>sas-sonar</module>
		<module>eclipse-startup</module>
		<module>eclipse-webapp</module>
		<module>ide-eclipse</module>
		<module>maven-helper</module>
    <module>jabox-standalone</module>
  </modules>
	<!--
		TODO <organization> <name>company name</name> <url>company url</url>
		</organization>
	-->
	<build>
		<plugins>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>2.0.2</version>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<licenses>
		<license>
			<name>The Apache Software License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
			<distribution>repo</distribution>
		</license>
	</licenses>
	<distributionManagement>
		<snapshotRepository>
			<id>snapshots</id>
			<name>Snapshots</name>
			<url>http://localhost:9090/nexus/content/repositories/snapshots/</url>
		</snapshotRepository>
		<repository>
			<id>releases</id>
			<name>Releases</name>
			<url>http://localhost:9090/nexus/content/repositories/releases/</url>
		</repository>
	</distributionManagement>
	<properties>
		<wicket.version>1.4.0</wicket.version>
		<jetty.version>6.1.4</jetty.version>
		<spring.version>2.5.4</spring.version>
	</properties>
</project>